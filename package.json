{
  "type": "module",
  "scripts": {
    "build": "tsc && npx tsc-alias --project tsconfig.json",
    "start": "node dist/server.js",
    "dev": "concurrently \"tsc --watch\" \"tsc-alias --project tsconfig.json --watch\" \"nodemon --watch dist dist/server.js\"",
    "restore": "node dist/utility/mongoRestore.js",
    "type-check": "tsc --noEmit --pretty",
    "type-check:watch": "tsc --noEmit --pretty --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
